pipeline {
    agent any 
    stages {
        stage('Build') { 
            steps {
               sh 'echo "this is the build"'
            }
        }
        stage('Test') { 
            steps {
               sh 'echo "this is the test"'
            }
        }
        stage('Deploy') { 
            steps {
                sh 'echo "this is the deploy"'
            }
        }
    }
}